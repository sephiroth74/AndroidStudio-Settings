      <option name="COMMAND" value="adb" />
      <option name="PARAMETERS" value="uninstall com.swisscom.swisscomTv" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
</toolSet>