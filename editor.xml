<application>
  <component name="CodeInsightSettings">
    <option name="completionCaseSensitive" value="2" />
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
    <option name="SHOW_FULL_SIGNATURES_IN_PARAMETER_INFO" value="true" />
    <option name="USE_INSTANCEOF_ON_EQUALS_PARAMETER" value="true" />
    <option name="USE_ACCESSORS_IN_EQUALS_HASHCODE" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="ERROR_STRIPE_MARK_MIN_HEIGHT" value="3" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="16" />
    <option name="FONT_FAMILY" value="Anonymice Nerd Font Mono" />
    <option name="LINE_SPACING" value="1.1" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
  </component>
  <component name="JavaCodeFoldingSettings">
    <option name="collapseConstructorGenericParameters" value="false" />
    <option name="collapseLambdas" value="false" />
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern icon="question" useCustomColors="true" pattern="\b(performance)\b.*">
      <option name="FOREGROUND" value="b0a7ff" />
      <option name="FONT_TYPE" value="3" />
    </pattern>
  </component>
</application>